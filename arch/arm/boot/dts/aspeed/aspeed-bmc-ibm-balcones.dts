		reg = <0x60>;
		#address-cells = <1>;
		#size-cells = <0>;
		gpio-controller;
		#gpio-cells = <2>;

		led@0 {
			reg = <0>;
			default-state = "keep";
			label = "front-sys-id0";
			retain-state-shutdown;
			type = <PCA955X_TYPE_LED>;
		};

		led@1 {
			reg = <1>;
			default-state = "keep";
			label = "front-check-log0";
			retain-state-shutdown;
			type = <PCA955X_TYPE_LED>;
		};

