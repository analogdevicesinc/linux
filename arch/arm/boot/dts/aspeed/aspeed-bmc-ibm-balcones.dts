	lcd-controller@62 {
		compatible = "ibm,op-panel";
		reg = <(0x62 | I2C_OWN_SLAVE_ADDRESS)>;
	};

	pressure-sensor@76 {
		compatible = "infineon,dps310";
		reg = <0x76>;
