&i2c5 {
	status = "okay";

	eeprom@52 {
		compatible = "atmel,24c64";
		reg = <0x52>;
	};

	led-controller@62 {
		compatible = "nxp,pca9551";
		reg = <0x62>;
		#address-cells = <1>;
		#size-cells = <0>;
		gpio-controller;
		#gpio-cells = <2>;

		led@0 {
