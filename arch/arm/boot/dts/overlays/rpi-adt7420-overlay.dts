/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2835", "brcm,bcm2708", "brcm,bcm2709", "brcm,bcm2711";

	fragment@1 {
		target = <&i2c1>;
		__overlay__ {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			adt7420: adt7420@4b {
				compatible = "adt7420";
				reg = <0x4b>;
			};
		};
	};
	__overrides__ {
		i2c_address = <&adt7420>,"reg:0";
	};
};
