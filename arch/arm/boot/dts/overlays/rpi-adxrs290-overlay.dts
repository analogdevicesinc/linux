/dts-v1/;
/plugin/;

/ {
	compatible = "brcm,bcm2836", "brcm,bcm2835";
};

&spi0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	adxrs290@0 {
		compatible = "adi,adxrs290";
		reg = <0>;
		spi-max-frequency = <1000000>;
		spi-cpha;
		spi-cpol;
		interrupts = <19 1>;
		interrupt-parent = <&gpio>;
	};
};

&spidev0 {
	status = "disabled";
};
