/dts-v1/;
/include/ "zynq-adrv9364-z7020.dtsi"


&axi_i2c0 {
	ad7291-bob@2C {
		compatible = "adi,ad7291";
		reg = <0x2c>;
	};

	eeprom@50 {
		compatible = "at24,24c32";
		reg = <0x50>;
	};
};

/ {
	leds {
		compatible = "gpio-leds";
		led0 {
			label = "led0:green";
			gpios = <&gpio0 58 0>;
		};

		led1 {
			label = "led1:green";
			gpios = <&gpio0 59 0>;
		};

		led2 {
			label = "led2:green";
			gpios = <&gpio0 60 0>;
		};

		led3 {
			label = "led3:green";
			gpios = <&gpio0 61 0>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		autorepeat;

		pb0 {
			label = "Left";
			linux,code = <105>;
			gpios = <&gpio0 54 0>;
		};

		pb1 {
			label = "Right";
			linux,code = <106>;
			gpios = <&gpio0 55 0>;
		};

		pb2 {
			label = "Up";
			linux,code = <103>;
			gpios = <&gpio0 56 0>;
		};

		pb3 {
			label = "Down";
			linux,code = <108>;
			gpios = <&gpio0 57 0>;
		};

		sw0 {
			label = "SW0";
			linux,input-type = <5>;
			linux,code = <0>;
			gpios = <&gpio0 62 0>;
		};

		sw1 {
			label = "SW1";
			linux,input-type = <5>;
			linux,code = <1>;
			gpios = <&gpio0 63 0>;
		};

		sw2 {
			label = "SW2";
			linux,input-type = <5>;
			linux,code = <2>;
			gpios = <&gpio0 64 0>;
		};

		sw3 {
			label = "SW3";
			linux,input-type = <5>;
			linux,code = <3>;
			gpios = <&gpio0 65 0>;
		};
	};
};
