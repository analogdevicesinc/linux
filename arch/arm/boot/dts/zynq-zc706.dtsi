/include/ "zynq.dtsi"

/ {
	model = "Xilinx Zynq ZC706";
	memory {
		device_type = "memory";
		reg = <0x00000000 0x40000000>;
	};

	chosen {
		bootargs = "console=ttyPS0,115200 root=/dev/mmcblk0p2 rw earlyprintk rootfstype=ext4 rootwait devtmpfs.mount=0";
		linux,stdout-path = "/amba@0/uart@E0001000";
	};

	leds {
		compatible = "gpio-leds";
		ds8 {
			label = "ds12:green";
			gpios = <&gpio 61 0>;
		};

		ds9 {
			label = "ds15:green";
			gpios = <&gpio 62 0>;
		};

		ds10 {
			label = "ds16:green";
			gpios = <&gpio 63 0>;
		};

		ds35 {
			label = "ds17:green";
			gpios = <&gpio 64 0>;
		};
	};

	gpio_keys {
		compatible = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		autorepeat;

		sw7 {
			label = "Left";
			linux,code = <105>; // Left
			gpios = <&gpio 58 0>;
		};

		sw8 {
			label = "Right";
			linux,code = <106>; // Right
			gpios = <&gpio 60 0>;
		};

		sw9 {
			label = "Select";
			linux,code = <28>; // Enter
			gpios = <&gpio 59 0>;
		};
	};
};

&eth {
	phy-handle = <&phy0>;
	phy-mode = "rgmii-id";

	phy0: phy@7 {
		compatible = "marvell,88e1116r";
		device_type = "ethernet-phy";
		reg = <0x7>;
	};
};
