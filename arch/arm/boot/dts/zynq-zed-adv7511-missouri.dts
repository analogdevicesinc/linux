			#address-cells = <1>;
			#size-cells = <0>;
			spi-max-frequency = <1000000>;
		};
		admv1455: admv1455@4 {
			compatible = "adi,admv1355";
			reg = <4>;
			#address-cells = <1>;
			#size-cells = <0>;
			spi-max-frequency = <1000000>;
			//cen-gpios = <&fmc_gpio 32 GPIO_ACTIVE_HIGH>;
			//reset-gpios = <&fmc_gpio 7 GPIO_ACTIVE_HIGH>;
		};
		admv8809_5: admv8809_5@5 {
