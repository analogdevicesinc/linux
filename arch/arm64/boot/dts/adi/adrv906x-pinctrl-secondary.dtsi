#include <dt-bindings/pinctrl/pinctrl-adi-adrv906x-io-pad.h>
#include "adrv906x_def.h"

&pinctrl_secondary {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_secondary_hog>;

	pinctrl_secondary_hog: secondary-hog-grp {
	};

	pinctrl_secondary_eth_recov_clk: secondary-eth-recov-clk-grp {
		adi,pins = <
		/* Pin 134: Ethernet recovered clock */
		ETHERNET_RECOVERED_CLK_PIN (ADI_CONFIG_NO_PULL | ADI_CONFIG_DRIVE_STRENGTH_4)
		>;
	};
};
