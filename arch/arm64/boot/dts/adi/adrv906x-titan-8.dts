/* For now, Titan-8 is identical to Denali-8 except for the changes below */
#include "adrv906x-denali-8.dts"

/ {
	model = "ADI ADRV906X Titan 8T8R Evaluation Kit";
	compatible = "adi,adrv906x-titan-8", "adi,adrv906x";
};

&dac0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_pwm12>;
};

&sec_dac0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_secondary_pwm8>;
};

