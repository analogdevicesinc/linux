				MX8MQ_IOMUXC_SD1_DATA0_GPIO2_IO2		0x00
				MX8MQ_IOMUXC_SD1_DATA1_GPIO2_IO3		0x00
				MX8MQ_IOMUXC_SD1_DATA2_GPIO2_IO4		0x00
				MX8MQ_IOMUXC_SD1_DATA3_GPIO2_IO5		0x00
				MX8MQ_IOMUXC_SD1_DATA4_GPIO2_IO6		0x00
				MX8MQ_IOMUXC_SD1_DATA5_GPIO2_IO7		0x00
				MX8MQ_IOMUXC_SD1_DATA6_GPIO2_IO8		0x00
				MX8MQ_IOMUXC_SD1_DATA7_GPIO2_IO9		0x00
