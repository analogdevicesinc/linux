		pinctrl_modem_reset: modem_resetgrp {
			fsl,pins = <
				/* BT_REG_ON */
				MX8MM_IOMUXC_SD1_DATA4_GPIO2_IO6		0x19
			>;
		};

		pinctrl_gpio_leds: gpioledgrp {
			fsl,pins = <
