/dts-v1/;

#include "../freescale/fsl-imx8mm.dtsi"

/ {
	model = "Emcraft i.MX8MM SOM board";
	compatible = "emcraft,imx8mm-som", "fsl,imx8mm";

	chosen {
		bootargs = "console=ttymxc1,115200 earlycon=ec_imx6q,0x30890000,115200";
		stdout-path = &uart2;
	};

	psci {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_pwr_en>;
		suspend-gpios = <&gpio1 8 GPIO_ACTIVE_LOW>;
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_leds>;
		status = "okay";

		led1 {
			label = "LED 1";
			gpios = <&gpio2 11 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
		led2 {
			label = "LED 2";
			gpios = <&gpio3 9 GPIO_ACTIVE_HIGH>;
			default-state = "off";
		};
	};

	modem_reset: modem-reset {
		compatible = "gpio-reset";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_modem_reset>;
		reset-gpios = <&gpio2 6 GPIO_ACTIVE_LOW>;
		reset-delay-us = <2000>;
		reset-post-delay-ms = <40>;
		#reset-cells = <0>;
		status = "okay";
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;
	};

	sound_osc: oscillator {
		compatible = "fixed-clock";
		#clock-cells = <1>;
		clock-frequency  = <12000000>;
		clock-output-names = "sound_osc";
	};

	sound-nau8820 {
		compatible = "fsl,imx-audio-wm8985";
		model = "nau8820-audio";
		audio-cpu = <&sai3>;
		audio-codec = <&nau8820_codec>;
	};

	sound-spdif {
		compatible = "fsl,imx-audio-spdif";
		model = "imx-spdif";
		spdif-controller = <&spdif1>;
		spdif-out;
		spdif-in;
	};

	sound-micfil {
		compatible = "fsl,imx-audio-micfil";
		model = "imx-audio-micfil";
		cpu-dai = <&micfil>;
	};

	m4ctrl {
		compatible = "fsl,imx-m4ctrl";
		reg = <0x0 0x7e0000 0x0 0x40000>;
		clocks = <&clk IMX8MM_CLK_M4_CG>;
		clock-names = "m4_clk";
	};
};

&clk {
	assigned-clocks = <&clk IMX8MM_AUDIO_PLL1>;
	assigned-clock-rates = <786432000>;
};

&iomuxc {
	imx8mm-som {
		pinctrl_pwr_en: pwren {
			fsl,pins = <
