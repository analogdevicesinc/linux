/dts-v1/;
#include "fsl-ls1028a-rdb.dts"

&enetc_port0 {
	/* Leave the port with SGMII in-band autoneg enabled */
	/delete-property/ phy-handle;
};

&enetc_port2 {
	fixed-link {
		/delete-property/ pause;
	};
};

/delete-node/ &enetc_mdio_pf3;

/* l2switch ports */
&mscc_felix_port0 {
	/delete-property/ phy-handle;
};

&mscc_felix_port1 {
	/delete-property/ phy-handle;
};

&mscc_felix_port2 {
	/delete-property/ phy-handle;
};

&mscc_felix_port3 {
	/delete-property/ phy-handle;
};

&mscc_felix_port4 {
	fixed-link {
		/delete-property/ pause;
	};
};

&mscc_felix_port5 {
	status = "okay";
	fixed-link {
		/delete-property/ pause;
	};
};

&enetc_port3 {
	status = "okay";
	fixed-link {
		/delete-property/ pause;
	};
};
