		reg = <0x56090000 0x10000>;
		clocks = <&dc0_prg5_lpcg 0>,
			 <&dc0_prg5_lpcg 1>;
		clock-names = "rtram", "apb";
		power-domains = <&pd IMX_SC_R_DC_0>;
		status = "disabled";
	};

