		bit-offset = <0 16>;
		clock-output-names = "dc0_prg4_lpcg_rtram_clk",
				     "dc0_prg4_lpcg_apb_clk";
		power-domains = <&pd IMX_SC_R_DC_0>;
	};

	dc0_prg5_lpcg: clock-controller@5601003c {
		compatible = "fsl,imx8qxp-lpcg";
		reg = <0x5601003c 0x4>;
		#clock-cells = <1>;
		clocks = <&dc0_axi_ext_clk>,
