		reg = <0x560e0000 0x10000>;
		fsl,sc-resource = <IMX_SC_R_DC_0_BLIT1>;
		fsl,prgs = <&dc0_prg2>, <&dc0_prg1>;
		clocks = <&dc0_dpr0_lpcg 0>,
			 <&dc0_dpr0_lpcg 1>,
			 <&dc0_rtram0_lpcg 0>;
