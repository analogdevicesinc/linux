		bit-offset = <0 16>;
		clock-output-names = "dc0_prg1_lpcg_rtram_clk",
				     "dc0_prg1_lpcg_apb_clk";
		power-domains = <&pd IMX_SC_R_DC_0>;
	};

	dc0_prg2_lpcg: clock-controller@56010028 {
		compatible = "fsl,imx8qxp-lpcg";
