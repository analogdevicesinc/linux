#include "imx8mp-evk.dts"

/ {
	model = "NXP i.MX8MPlus DDR4 EVK board";
};

&flexspi {
	status = "disabled";
};

&usdhc3 {
	status = "disabled";
};
&lcdif3 {
	thres-low  = <2 3>;             /* (FIFO * 2 / 3) */
	thres-high = <3 3>;             /* (FIFO * 3 / 3) */
	status = "okay";
};
