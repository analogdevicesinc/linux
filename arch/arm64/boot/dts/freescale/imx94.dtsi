				compatible = "fsl,imx94-sai", "fsl,imx95-sai";
				reg = <0x42660000 0x10000>;
				interrupts = <GIC_SPI 102 IRQ_TYPE_LEVEL_HIGH>;
				clocks = <&scmi_clk IMX94_CLK_BUSWAKEUP>, <&dummy>,
					<&scmi_clk IMX94_CLK_SAI3>, <&dummy>, <&dummy>;
