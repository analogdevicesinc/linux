					 <&scmi_clk IMX94_CLK_BUSWAKEUP>;
				clock-names = "per", "ipg";
				dmas = <&edma4 4 0 0>, <&edma4 5 0 FSL_EDMA_RX>;
				dma-names = "tx", "rx";
				status = "disabled";
