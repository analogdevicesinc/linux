/dts-v1/;

#include "imx95-19x19-evk.dts"

/{
	interrupt-parent = <&gic>;

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
	};
};

&linux_cma {
	size = <0 0x4000000>;
	alloc-ranges = <0 0x80000000 0 0x20000000>;
};

&pcie_4ca00000 {
	linux,pci-domain = <2>;
};

&pcie_4cb00000 {
	linux,pci-domain = <3>;
};

/delete-node/ &isi;

&usdhc1 {
	status = "disabled";
};

&usdhc2 {
	status = "okay";
};
