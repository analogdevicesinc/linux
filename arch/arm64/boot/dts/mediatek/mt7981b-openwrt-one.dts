/dts-v1/;

#include "mt7981b.dtsi"

/ {
	compatible = "openwrt,one", "mediatek,mt7981b";
	model = "OpenWrt One";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory@40000000 {
		reg = <0 0x40000000 0 0x40000000>;
		device_type = "memory";
	};
};

&uart0 {
	status = "okay";
};
