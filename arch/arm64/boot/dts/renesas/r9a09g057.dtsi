				     <GIC_SPI 95  IRQ_TYPE_EDGE_RISING>,
				     <GIC_SPI 96  IRQ_TYPE_EDGE_RISING>,
				     <GIC_SPI 97  IRQ_TYPE_EDGE_RISING>,
