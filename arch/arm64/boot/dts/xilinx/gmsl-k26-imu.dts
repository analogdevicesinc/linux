	status = "okay";
	spi-3wire = <0>;
	spi-max-frequency = <2000000>;

	adis16500: adis16500@0 {
                compatible = "adi,adis16500";
		reg = <0>;
		interrupts = <0 92 1>;
		spi-max-frequency = <2000000>;
		reset-gpios = <&gpio 89 GPIO_ACTIVE_LOW>;	
                spi-cpol;
                spi-cpha;
	};
};

