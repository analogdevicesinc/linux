#define GMSL_CAMERA_0
#endif
#if (GMSL_CAMERA_NUM >= 2)
#define GMSL_CAMERA_1
#endif
#if (GMSL_CAMERA_NUM >= 3)
#define GMSL_CAMERA_2
#endif
