CONFIG_LOCALVERSION_AUTO=y
CONFIG_SLAB=y
CONFIG_MODULE_FORCE_LOAD=y
CONFIG_MODVERSIONS=y
CONFIG_BLK_DEV_RAM=y
CONFIG_PID_IN_CONTEXTIDR=y
CONFIG_EXPERT=y
CONFIG_CHECKPOINT_RESTORE=y
CONFIG_PROC_KCORE=y
