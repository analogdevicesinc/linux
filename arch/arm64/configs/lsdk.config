CONFIG_VHOST_NET=y
CONFIG_KVM_ARM_MAX_VCPUS=8

# DPAA1 networking
# disable the DPAA1 upstream driver
CONFIG_FSL_DPAA=n
CONFIG_FSL_FMAN=n
CONFIG_FSL_DPAA_ETH=n
