# PHY drivers for Nuvoton MA35 platforms
#
config PHY_MA35_USB
	tristate "Nuvoton MA35 USB2.0 PHY driver"
	depends on ARCH_MA35 || COMPILE_TEST
	depends on OF
	select GENERIC_PHY
	help
	  Enable this to support the USB2.0 PHY on the Nuvoton MA35
	  series SoCs.
